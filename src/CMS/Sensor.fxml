<AnchorPane minHeight="600.0" minWidth="700.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CMS.SensorController">
   <children>
      <Button fx:id="SensorAdd" layoutX="14.0" layoutY="534.0" mnemonicParsing="false" onAction="#MenuWindow" prefHeight="52.0" prefWidth="168.0" text="Sensor Add" />
      <Button fx:id="SensorUpdate" layoutX="373.0" layoutY="471.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="168.0" text="Sensor Update" />
      <Button fx:id="SensorDelete" layoutX="182.0" layoutY="471.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="168.0" text="Sensor Delete" />
      <Button layoutX="508.0" layoutY="534.0" mnemonicParsing="false" onAction="#MenuWindow" prefHeight="52.0" prefWidth="168.0" text="Menu" />
      <TextField fx:id="SenID" layoutX="45.0" layoutY="36.0" promptText="Sensor ID" />
      <TextField fx:id="SenName" layoutX="202.0" layoutY="36.0" promptText="Sensor Name" />
      <TextField fx:id="SensorModel" layoutX="360.0" layoutY="36.0" promptText="Model" />
      <TextField fx:id="SensorSerialNo" layoutX="520.0" layoutY="36.0" promptText="Serial Number" />
